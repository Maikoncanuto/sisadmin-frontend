{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-sisadmin": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "sisadmin",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "spec": false
        },
        "@schematics/angular:class": {
          "spec": false
        },
        "@schematics/angular:directive": {
          "spec": false
        },
        "@schematics/angular:guard": {
          "spec": false
        },
        "@schematics/angular:module": {
          "spec": false
        },
        "@schematics/angular:pipe": {
          "spec": false
        },
        "@schematics/angular:service": {
          "spec": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-sisadmin",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/lib/chosen/chosen.css",
              "src/assets/css/lib/datatable/buttons.bootstrap.min.css",
              "src/assets/css/lib/datatable/buttons.dataTables.min.css",
              "src/assets/css/lib/datatable/dataTables.bootstrap.min.css",
              "src/assets/css/lib/vector-map/jqvmap.min.css",
              "src/assets/css/cs-skin-elastic.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/js/lib/chosen/chosen.proto.js",
              "src/assets/js/lib/data-table/buttons.bootstrap.min.js",
              "src/assets/js/lib/data-table/buttons.colVis.min.js",
              "src/assets/js/lib/data-table/buttons.flash.min.js",
              "src/assets/js/lib/data-table/buttons.html5.min.js",
              "src/assets/js/lib/data-table/buttons.print.min.js",
              "src/assets/js/lib/data-table/dataTables.bootstrap.min.js",
              "src/assets/js/lib/data-table/dataTables.buttons.min.js",
              "src/assets/js/lib/data-table/datatables.min.js",
              "src/assets/js/lib/data-table/jquery.dataTables.min.js",
              "src/assets/js/lib/data-table/jszip.min.js",
              "src/assets/js/lib/data-table/pdfmake.min.js",
              "src/assets/js/lib/data-table/vfs_fonts.js",
              "node_modules/@amcharts/amcharts4/core.js",
              "node_modules/@amcharts/amcharts4/maps.js",
              "node_modules/@amcharts/amcharts4/charts.js",
              "node_modules/@amcharts/amcharts4/themes/amcharts.js",
              "src/assets/js/dashboard.js",
              "src/assets/js/main.js",
              "src/assets/js/vmap.sampledata.js",
              "src/assets/js/lib/gmap/gmapApi.js",
              "src/assets/js/widgets.js",
              "src/assets/js/lib/gmap/gmaps.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-sisadmin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-sisadmin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-sisadmin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-sisadmin-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "app-sisadmin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-sisadmin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "app-sisadmin",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
